extends ../layout

block main
  section(class="section")
    include navbar
  section(class="section")
    include level
    div(class="container")
      div(class="table-container")
        table(class="table is-fullwidth")
            thead
              tr
                th 이름
                th LDAP ID
                th EMAIL
            tbody
              each list in userdb
                  tr
                    td= list.user_name
                    td= list.user_ldap
                    td= list.user_email
